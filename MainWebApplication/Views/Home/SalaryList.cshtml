@model IEnumerable<MainWebApplication.Models.SalaryMaster>
@if (Model.Count() == 0)
{
    <main>
    <div class="container">

      <section class="section error-404 min-vh-100 d-flex flex-column align-items-center justify-content-center">
        <h1>404</h1>
        <h2>Похоже Вы еще не настроили зарплату мастерам.</h2>
        <a class="btn" href="/Home/CreateSalaryMaster">Настроить зарплату</a>
      </section>

    </div>
  </main><!-- End #main -->
}
else
{
    <div>
        <a href="/Home/CreateSalaryMaster">Настроить зарплату</a>
    </div>
    <table class="table">
    <thead>
        <tr>
             <th>
                @Html.DisplayNameFor(model => model.AspNetUser.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Service.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Service.Price)
            </th>
            <th>
               @Html.DisplayNameFor(model => model.SalaryMasterStr)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
             <td>
                @Html.DisplayFor(modelItem => item.AspNetUser.FullName)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Service.Name)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Service.Price) тенге
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SalaryMasterStr)
            </td>
        </tr>
}
    </tbody>
</table>
}